{
  "name": "react-webpack-funpack",
  "version": "1.0.0",
  "description": "Node and ReactJS on the dancefloor; universally brought together by Typescript, Webpack, Express and Parse Server",
  "scripts": {
    "dev": "npm run compile && cross-env DEBUG=app:* NODE_ENV=development node ./bin/start.js",
    "dev:attach": "npm run compile && cross-env DEBUG=app:* NODE_ENV=development DEBUGGING=true node ./bin/start.js",
    "build": "cross-env NODE_ENV=production DEBUG=app:* npm-run-all compile build:prod",
    "start": "cross-env DEBUG=app:* npm run kill && cross-env NODE_ENV=production DEBUG=app:*  node bin/server.js",
    "lint": "tslint -s node_modules/custom-tslint-formatters/formatters -t grouped 'src/**/*.ts?(x)' 'bin/**/*.ts?(x)'",
    "lint:vscode": "tslint -s node_modules/custom-tslint-formatters/formatters -t vscode 'src/**/*.ts?(x)' 'bin/**/*.ts?(x)'",
    "compile": "cross-env DEBUG=app:* node ./tsc-compile.js",
    "test": "npm run compile && cross-env NODE_ENV=test karma start",
    "typings": "typings install",
    "postinstall": "typings install",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanvale/react-webpack-funpack.git"
  },
  "bugs": {
    "url": "https://github.com/ayoubdev/react-webpack-funpack/issues"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react-router",
    "boilerplate",
    "template",
    "typescript",
    "transpilation",
    "isomorphic",
    "universal",
    "server side rendering",
    "client side rendering",
    "starter",
    "kit",
    "starter-kit"
  ],
  "main": "bin/server.js",
  "engines": {
    "node": "6.3.0",
    "npm": "3.10.5"
  },
  "config": {
    "ghooks": {
      "pre-commit": "",
      "pre-push": ""
    },
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-customizable.js"
    }
  },
  "author": "Nathan Vale <hi@nathanvale.com>",
  "license": "MIT",

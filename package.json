{
  "name": "reactjs-launch-stack",
  "version": "1.0.0",
  "description": "A React.js stack typescript isomorphic template with Webpack and react-router",
  "scripts": {
    "dev": "npm run compile && cross-env DEBUG=app:* NODE_ENV=development node ./bin/start.js",
    "dev:attach": "npm run compile && cross-env DEBUG=app:* NODE_ENV=development DEBUGGING=true node ./bin/start.js",
    "build": "cross-env NODE_ENV=production DEBUG=app:* npm-run-all compile build:prod",
    "start": "cross-env DEBUG=app:* npm run kill && cross-env NODE_ENV=production DEBUG=app:*  node bin/server.js",
    "lint": "tslint -s node_modules/custom-tslint-formatters/formatters -t grouped 'src/**/*.ts?(x)' 'config/**/*.ts?(x)' 'bin/**/*.ts?(x)'",
    "compile": "cross-env DEBUG=app:* node ./tsc-compile.js",
    "test": "npm run compile && cross-env NODE_ENV=test karma start",
    "postinstall": "typings install && npm run build",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayoubdev/reactjs-typescript-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/ayoubdev/reactjs-typescript-starterkit/issues"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react-router",
    "boilerplate",
    "template",
    "typescript",
    "transpilation",
    "isomorphic",
    "universal",
    "server side rendering",
    "client side rendering",
    "starter",
    "kit",
    "starterkit",
    "starter-kit"
  ],
  "main": "bin/server.js",
  "engines": {
    "node": "4.4.0",
    "npm": "3.8.7"
  },

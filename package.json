{
  "name": "reactjs-typescript-starterkit",
  "version": "1.0.0",
  "description": "A React.js typescript isomorphic template with Webpack and react-router",
  "scripts": {
    "test": "better-npm-run test",
    "clean": "node ./bin/clean.js",
    "postinstall": "npm run ts:init",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "ts:init": "typings install",
    "server:clientside": "node ./bin/server/clientSideRendering.js",
    "server:serverside": "node ./bin/server/serverSideRendering.js",
    "tsc:webpack": "tsc ./config/webpack.clientside.dev.ts ./config/webpack.clientside.prod.ts ./config/webpack.constants.ts ./config/webpack.serverside.ts ./config/karma.config.ts ./typings/index.d.ts ./src/shared/global.d.ts  ",
    "dev:build": "npm run tsc:webpack && webpack -d --colors --config ./config/webpack.clientside.dev.js",
    "dev": " better-npm-run dev",
    "prod:build:clientside": "npm run tsc:webpack && webpack -p --progress --colors --config ./config/webpack.clientside.prod.js",
    "prod:build:serverside": "npm run tsc:webpack && webpack -p --progress --colors --display-error-details --config ./config/webpack.serverside.js",
    "prod:deploy:clientside": "npm run prod:build:clientside && npm run server:clientside",
    "prod:deploy:serverside": "npm run prod:build:serverside && npm run server:serverside",
    "prod:deploy": "npm run prod:deploy:serverside"
  },
  "betterScripts": {
    "dev": {
      "command": "npm run tsc:webpack && node ./bin/server/clientSideRendering.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "npm run tsc:webpack && node ./node_modules/karma/bin/karma start config/karma.config.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayoubdev/reactjs-typescript-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/ayoubdev/reactjs-typescript-starterkit/issues"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react-router",
    "boilerplate",
    "template",
    "typescript",
    "transpilation",
    "isomorphic",
    "server side rendering",
    "client side rendering",
    "starter",
    "kit",
    "starterkit",
    "starter-kit"
  ],
  "author": "Ayoub ADIB <ayoub.development@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "better-npm-run": "0.0.8",
    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "clean-webpack-plugin": "^0.1.9",
    "compression": "^1.6.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.3.2",
    "del": "^2.2.0",
    "es6-object-assign": "^1.0.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "fs": "0.0.2",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.19.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.9.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-helmet": "^2.2.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.2.0",
    "react-router-redux": "dlmr/react-router-redux#fix-inital-server-load-dist",
    "redux": "^3.0.0",
    "redux-observable": "^0.5.0",
    "rxjs": "^5.0.0-beta.8",
    "sass-loader": "^3.2.0",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.0",
    "ts-loader": "0.8.2",
    "tslint": "^3.10.2",
    "typescript": "^1.8.7",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",

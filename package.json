{
  "name": "reactjs-launch-stack",
  "version": "1.0.0",
  "description": "A React.js stack typescript isomorphic template with Webpack and react-router",
  "scripts": {
    "dev": "npm-run-all --parallel compile:watch build:dev-client build:dev-server start:dev-server",
    "build:dev-server": "cross-env NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-server.js  --watch --colors --display-error-details",
    "build:dev-client": "node ./webpack/webpack.server.js",
    "start:dev-server": "npm run delay && nodemon",
    "development-server": "npm-run-all delay development-server-start",
    "delay": "node ./bin/sleep.js 10000",
    "build:dev": "npm run compile && cross-env NODE_ENV=development && node bin/server-dev-client.js",
    "build:prod": "npm run compile && cross-env NODE_ENV=production webpack --colors --config ./webpack/webpack.config.prod.js",
    "build": "npm run build:prod",
    "lint": "tslint -s node_modules/custom-tslint-formatters/formatters -t grouped 'src/**/*.ts?(x)' 'config/**/*.ts?(x)' 'bin/**/*.ts?(x)'",
    "lint:vscode": "tslint -s node_modules/custom-tslint-formatters/formatters -t vscode 'src/**/*.ts?(x)' 'config/**/*.ts?(x)' 'bin/**/*.ts?(x)'",
    "compile": "npm run clean && tsc",
    "compile:watch": "npm run compile -- -w",
    "clean": "tsc typings/index.d.ts bin/clean.ts && node ./bin/clean.js",
    "test": "npm run compile && cross-env NODE_ENV=test karma start",
    "postinstall": "typings install && npm run build",
    "start": "cross-env NODE_ENV=production node bin/server.js",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayoubdev/reactjs-typescript-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/ayoubdev/reactjs-typescript-starterkit/issues"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react-router",
    "boilerplate",
    "template",
    "typescript",
    "transpilation",
    "isomorphic",
    "universal",
    "server side rendering",
    "client side rendering",
    "starter",
    "kit",
    "starterkit",
    "starter-kit"
  ],
  "main": "bin/server.js",
  "engines": {
    "node": "4.4.0",
    "npm": "3.8.7"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": ""
    },
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-customizable.js"
    }
  },
  "author": "Ayoub ADIB <ayoub.development@gmail.com>",
